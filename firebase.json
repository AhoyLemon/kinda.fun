{
  "hosting": {
    "site": "kinda-fun",
    "public": ".output/public",
    "ignore": [],
    "redirects": [
      {
        "source": "/cameo.html",
        "destination": "/cameo",
        "type": 301
      },
      {
        "source": "/guillotine.html",
        "destination": "/guillotine",
        "type": 301
      },
      {
        "source": "/invalid.html",
        "destination": "/invalid",
        "type": 301
      },
      {
        "source": "/meeting.html",
        "destination": "/meeting",
        "type": 301
      },
      {
        "source": "/megachurch.html",
        "destination": "/megachurch",
        "type": 301
      },
      {
        "source": "/pretend.html",
        "destination": "/pretend",
        "type": 301
      },
      {
        "source": "/sisyphus.html",
        "destination": "/sisyphus",
        "type": 301
      },
      {
        "source": "/wrongest.html",
        "destination": "/wrongest",
        "type": 301
      },
      {
        "source": "/stats.html",
        "destination": "/stats",
        "type": 301
      },
      {
        "source": "/home.html",
        "destination": "/",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": ["node_modules", ".git", "firebase-debug.log", "firebase-debug.*.log", "*.local"]
    }
  ]
}
