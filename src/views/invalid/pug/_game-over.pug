      .award(v-if="computedCheevos.bestCracker && computedCheevos.bestCracker.name != 'TIE'")
        figure.icon
          img(src="/svg/invalid/noun_punk_pig_227290.svg" alt="cyberpunk pig")
        .award-name Supercracker
        .recipient {{computedCheevos.bestCracker.name}}
        .description Cracked {{computedCheevos.bestCracker.cracks}} passwords

      .award(v-if="computedCheevos.mostCracked && computedCheevos.mostCracked.name != 'TIE'")
        figure.icon
          img(src="/svg/invalid/doomed_pig.svg" alt="victim pig")
        .award-name Supervictim
        .recipient {{computedCheevos.mostCracked.name}}
        .description Password cracked {{computedCheevos.mostCracked.cracked}} times.

      .award(v-if="computedCheevos.mostSelfPwns && computedCheevos.mostSelfPwns.name != 'TIE'")
        figure.icon
          img(src="/svg/invalid/noun_derp_pig_227284.svg" alt="derp pig")
        .award-name SELF-PWNED
        .recipient {{computedCheevos.mostSelfPwns.name}}
        .description Cracked their own password 
          span(v-if="computedCheevos.mostSelfPwns.selfPwn > 1")  {{computedCheevos.mostSelfPwns.selfPwn}} times

    
      .award(v-if="computedCheevos.mostCrashes && computedCheevos.mostCrashes.name != 'TIE'")
        figure.icon
          img(src="/svg/invalid/noun_dead_pig_227289.svg" alt="crash happy pig")
        .award-name CRASH HAPPY
        .recipient {{computedCheevos.mostCrashes.name}}
        .description Crashed the system
          span(v-if="computedCheevos.mostCrashes.crashesCaused > 1")  {{computedCheevos.mostCrashes.crashesCaused}} times

      .award(v-if="computedCheevos.mostAttempts && computedCheevos.mostAttempts.name != 'TIE'")
        figure.icon
          img(src="/svg/invalid/noun_suspicious_pig_227293.svg" alt="quick finger pig")
        .award-name Quickie Fingers
        .recipient {{computedCheevos.mostAttempts.name}}
        .description Attempted {{computedCheevos.mostAttempts.passwordAttempts}} passwords

